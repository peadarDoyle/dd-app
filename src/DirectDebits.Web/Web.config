  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="ClientId" value="{XXXXXX}" />
    <add key="ClientSecret" value="" />
    <add key="BaseUri" value="https://start.exactonline.co.uk" />
    <add key="TokenEndpoint" value="https://start.exactonline.co.uk/api/oauth2/token" />
    <add key="UserInfoEndpoint" value="https://start.exactonline.co.uk/api/v1/current/Me" />
    <add key="AuthorizeEndpoint" value="https://start.exactonline.co.uk/api/oauth2/auth" />
    <add key="XMLUploadPath" value="/docs/XMLUpload.aspx" />
    <add key="PageSize" value="10" />
    <add key="TokenPassPhrase" value="ohjt65496q88ui8py4984w4fgd02v13xshesj6re" />
    <add key="UseSeq" value="true" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <customErrors mode="RemoteOnly" redirectMode="ResponseRewrite" defaultRedirect="~/500.aspx">
      <error statusCode="404" redirect="~/404.aspx" />
      <error statusCode="500" redirect="~/500.aspx" />
    </customErrors>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.5.2" />
    <httpHandlers>
      <add path="*.less" verb="GET" type="dotless.Core.LessCssHttpHandler, dotless.Core" />
    </httpHandlers>
    <globalization uiCulture="en-IE" culture="en-IE" />
    <sessionState mode="InProc" timeout="720" />
    <pages>
      <namespaces>
        <add namespace="MvcPaging" />
      </namespaces>
    </pages>
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
    <handlers>
      <add name="dotless" path="*.less" verb="GET" type="dotless.Core.LessCssHttpHandler,dotless.Core" resourceType="File" preCondition="" />
    </handlers>
    <httpErrors errorMode="DetailedLocalOnly">
      <remove statusCode="404" />
      <error statusCode="404" path="404.html" responseMode="File" />
      <remove statusCode="500" />
      <error statusCode="500" path="500.html" responseMode="File" />
    </httpErrors>
    <validation validateIntegratedModeConfiguration="false" />
  </system.webServer>
